<!-- UI Entities Metadata - Field Definitions for User Interface Components -->

<!-- UI IMAGE DATA ENTITY - Manages image resources -->
<ENTITY NAME="UI.IMAGE.DATA" VERSION="1.00">
    <DATARANGE>
        <START />
        <END />
        <CUR_PAGE />
        <PAGE_ENTRIES />
        <TOTAL_PAGES />
        <TOTAL_ENTRIES />
        <SORT_LIST>
            <SORT SORT="">
                <NAME></NAME>
                <SORTTYPE></SORTTYPE>
            </SORT>
        </SORT_LIST>
        <FILTER_LIST>
            <FILTER>
                <NAME></NAME>
                <FILTERTYPE></FILTERTYPE>
                <BEGIN_VALUE></BEGIN_VALUE>
                <END_VALUE></END_VALUE>
            </FILTER>
        </FILTER_LIST>
    </DATARANGE>
    <LIST_PROPERTIES>
        <NAME />
    </LIST_PROPERTIES>
    <FIELDS>
        <ATTRIBUTEGROUP NAME="UI.IMAGE.ITEM">
            <FIELD NAME="IMAGE">
                <CAPTION>Picture</CAPTION>
                <MUSTFILL>MUSTFILL:FALSE</MUSTFILL>
                <MAXLENGTH />
                <DEFAULTVALUE />
                <VISIBLE />
                <EDITABLE />
                <POSITION />
                <COLUMNWIDTH />
                <DATATYPE>DATATYPE:BLOB</DATATYPE>
                <EDITHINT>Select the picture!</EDITHINT>
                <HELPDESCRIPTION>Shows you the picture!</HELPDESCRIPTION>
                <SYSTEMCOLUMN />
                <UR>RIGHT:DELETE</UR>
                <FREEZE />
            </FIELD>
            <FIELD NAME="IMAGE_IDENTIFIER">
                <CAPTION>Identifier</CAPTION>
                <MUSTFILL>MUSTFILL:FALSE</MUSTFILL>
                <MAXLENGTH>200</MAXLENGTH>
                <DEFAULTVALUE />
                <VISIBLE />
                <EDITABLE />
                <POSITION />
                <COLUMNWIDTH />
                <DATATYPE>DATATYPE:ALPHANUMERIC</DATATYPE>
                <EDITHINT>Enter the image identification!</EDITHINT>
                <HELPDESCRIPTION>Shows you the identification of the picture!</HELPDESCRIPTION>
                <SYSTEMCOLUMN />
                <UR>RIGHT:DELETE</UR>
                <FREEZE />
            </FIELD>
        </ATTRIBUTEGROUP>
    </FIELDS>
</ENTITY>

